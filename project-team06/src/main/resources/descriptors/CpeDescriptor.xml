<?xml version="1.0" encoding="UTF-8"?>
<cpeDescription>
	<collectionReader>
		<collectionIterator>
			<descriptor>
				<import location="ReaderDescriptor.xml" />
			</descriptor>
		</collectionIterator>
	</collectionReader>
	<casProcessors casPoolSize="5" processingUnitThreadCount="1">
	
		<casProcessor deployment="integrated" name="QuestionNormalization">
			<descriptor>
				<import location="QuestionNormalization.xml" />
			</descriptor>
			<filter></filter>
			<errorHandling>
				<maxConsecutiveRestarts action="terminate"
					value="100" />
				<errorRateThreshold action="terminate" value="0/10000" />
				<timeout max="10000" />
			</errorHandling>
		</casProcessor>
		
		<casProcessor deployment="integrated" name="GoPubMedServiceCall">
			<descriptor>
				<import location="GoPubMedServiceCall.xml" />
			</descriptor>
			<filter></filter>
			<errorHandling>
				<maxConsecutiveRestarts action="terminate"
					value="100" />
				<errorRateThreshold action="continue" value="0/10000" />
				<timeout max="10000" />
			</errorHandling>
		</casProcessor>
		
		<!--casProcessor deployment="integrated" name="CorrectRankingAlgo">
			<descriptor>
				<import location="CorrectRankingAlgo.xml" />
			</descriptor>
			<filter></filter>
			<errorHandling>
				<maxConsecutiveRestarts action="terminate"
					value="100" />
				<errorRateThreshold action="terminate" value="10/10000" />
				<timeout max="10000" />
			</errorHandling>
		</casProcessor-->
		
		<!--casProcessor deployment="integrated" name="NLPProcessDescriptor">
			<descriptor>
				<import location="NLPProcessDescriptor.xml" />
			</descriptor>
			<filter></filter>
			<errorHandling>
				<maxConsecutiveRestarts action="terminate"
					value="100" />
				<errorRateThreshold action="terminate" value="10/10000" />
				<timeout max="10000" />
			</errorHandling>
		</casProcessor-->
		

		
		
	</casProcessors>
	<cpeConfig>
		<numToProcess>-1</numToProcess>
		<deployAs>immediate</deployAs>
	</cpeConfig>
</cpeDescription>